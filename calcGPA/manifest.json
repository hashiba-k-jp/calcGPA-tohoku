{
    "manifest_version": 3,
    "name": "Calc GPA",
    "version": "1.3.1",

    "description": "GPA Calculator for Tohoku University Student Affairs Information System",
    "homepage_url": "https://qiita.com/kekeke-udon/items/bea9145e00597b1a3c5d",

    "content_scripts": [
        {
            "matches": [
                "https://www.srp.tohoku.ac.jp/sa/wssrlstr.do*",
                "https://gkms2.bureau.tohoku.ac.jp/sa/wssrlstr.do*"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "content.css"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources" : [
                "/*"
            ],
            "matches" : [
                "<all_urls>"
            ]
        }
    ]
}
